  "compilerOptions": {
    "strictPropertyInitialization": false,
    "strict": true,
    "experimentalDecorators": true,
    "paths": {
      // "@app/*": ["./src/*"],
      "@services/*": ["./src/services/*"],
			"@components/*": ["./src/core/components/*"],
